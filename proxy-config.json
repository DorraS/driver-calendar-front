{
  "/api/*": {
    "target": "http://localhost:1337/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/google/*": {
    "target": "https://maps.googleapis.com/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/google": ""
    },
    "changeOrigin": true
  }
  
}